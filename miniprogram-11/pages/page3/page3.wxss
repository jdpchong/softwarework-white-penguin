/* pages/page3/page3.wxss */
page{
  width: 100%;
  height: 100%;
}
.container{
  display: flex;
  flex-direction: column;
  height: 100%;
}

.nav{
  width: 100%;
  height: 100rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.item{
  font-size: 32rpx;
  color: black;
  height: 50rpx;
  width: 100rpx;
  line-height: 50rpx;
  align-self: center;
  text-align: center;
}

.active{
  background-color: rgb(120, 120, 247);
  border-radius: 10rpx;
  width:30% ;
  height:55% ;
}

.list{
  width: 100%;
  height: 100%;
  display: flex;
  background-color:white;
}

.detail{
  margin: auto;
}